page{font-family:PingFang SC,sans-serif;color:#000}.pet-container{background-color:#f4f5f9;min-height:100vh;display:flex;flex-direction:column}.pet-avatar{width:120rpx;height:120rpx;border-radius:50%;overflow:hidden}.pet-box{width:690rpx;margin:50px auto;border:1px solid #eee;border-radius:10rpx;box-shadow:0 0 30rpx rgba(0,0,0,.05)}.pet-list-container{display:flex;height:2em;justify-content:space-between}.pet-list-ele{position:relative;flex:1;text-align:center;min-width:0;padding:10rpx;align-items:center}.pet-pic-icon{position:absolute;left:160rpx;top:20rpx;width:20rpx;height:20rpx}.pet-text-cut{overflow:hidden;white-space:nowrap;text-overflow:ellipsis}.pet-master-list{height:90vh;background-color:#f4f5f9;padding:20rpx 0 0;display:flex;flex-direction:column}.pet-master{display:flex;margin:8rpx 16rpx;background-color:#fff;height:200rpx;border-radius:32rpx}.pet-avatar{flex:3;margin:40rpx 10rpx;height:120rpx;width:120rpx}.pet-introduction{flex:10;margin:20rpx 30rpx 20rpx 10rpx}.pet-online{width:3em;text-align:center;border-radius:20rpx;height:44rpx;background-color:#edf6f3}.pet-distance-order{position:relative;margin:4rpx 20rpx 4rpx 4rpx;flex:2;display:flex;flex-direction:column}.pet-distance{color:gray;font-size:30rpx;text-align:center;padding-top:15rpx}.pet-order{width:130rpx;height:50rpx;text-align:center;margin:30rpx auto;font-weight:700;font-size:28rpx;border-radius:40rpx;background-color:#b2d76e}.pet-wizard{position:fixed;bottom:0;left:0;right:0;height:100rpx;display:grid;background-color:#fff;grid-template-columns:20% 20% 20% 20% 20%;padding-bottom:20rpx}.pet-wizard-image{height:46rpx;margin:10rpx 0rpx 0rpx 50rpx}.pet-wizard-text{text-align:center;margin-right:6rpx;color:#dbdfe8;font-size:24rpx}.pet-unique{height:112rpx;margin:-52rpx 0rpx 0rpx 10rpx}
