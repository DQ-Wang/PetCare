page{font-family:PingFang SC,sans-serif;color:#000}.pet-container{background-color:#f4f5f9;min-height:100vh;display:flex;flex-direction:column}.pet-avatar{width:120rpx;height:120rpx;border-radius:50%;overflow:hidden}.pet-box{width:690rpx;margin:50px auto;border:1px solid #eee;border-radius:10rpx;box-shadow:0 0 30rpx rgba(0,0,0,.05)}.pet-list-container{display:flex;height:2em;justify-content:space-between}.pet-list-ele{position:relative;flex:1;text-align:center;min-width:0;padding:7px 8px;align-items:center}.pet-pic-icon{position:absolute;left:82px;top:12px;width:10px;height:10px}.pet-text-cut{overflow:hidden;white-space:nowrap;text-overflow:ellipsis}.pet-master-list{height:90vh;background-color:#f4f5f9;padding:10px 0 20px;display:flex;flex-direction:column}.pet-master{display:flex;margin:8px 8px 12px;background-color:#fff;height:100px;border-radius:16rpx}.pet-avatar{flex:3;margin:20px 5px;height:60px;width:60px}.pet-introduction{flex:10;margin:10px 15px 10px 5px}.pet-online{width:3em;text-align:center;border-radius:10px;height:22px;background-color:#edf6f3}.pet-distance-order{position:relative;margin:2px;flex:2;display:flex;flex-direction:column;margin-right:20rpx}.pet-distance{color:gray;font-size:15px;text-align:center;padding-top:15rpx}.pet-order{width:65px;height:25px;text-align:center;margin:30rpx auto;font-weight:700;font-size:14px;border-radius:20px;background-color:#b2d76e}.pet-wizard{position:fixed;bottom:0;left:0;right:0;height:50px;display:grid;background-color:#fff;grid-template-columns:20% 20% 20% 20% 20%;padding-bottom:10px}.pet-wizard-image{height:23px;margin:5px 0 0 25px}.pet-wizard-text{text-align:center;margin-right:3px;color:#dbdfe8;font-size:12px}.pet-unique{height:56px;margin:-26px 0 0 5px}
