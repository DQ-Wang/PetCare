page {
  font-family: "PingFang SC", sans-serif;


  color: #000;
}
.pet-container {
  background-color: #f4f5f9;
  min-height: 100vh;
  display: flex;
  flex-direction: column;
}
.pet-avatar {
  /* 头像类 */
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
}
.pet-box {
  width: 690rpx;
  /* 此处可修改margin距离 */
  margin: 50px auto;
  border: 1px solid #eee;
  border-radius: 10rpx;
  box-shadow: 0 0 30rpx rgba(0, 0, 0, 0.05);
}
.main-area {
  width: 92vw;
  height: 80vh;
  display: flex;
  flex-direction: column;
  margin: -200rpx auto 0 auto;
}
.box {
  background: white;
  border-radius: 16rpx;
}
.user-info {
  height: 140rpx;
  display: flex;
  padding: 20rpx;
}
.user-info .left-section {
  margin: auto 15rpx auto 0;
  flex: 1;
}
.user-info .middle-section {
  flex: 3;
}
.user-info .middle-section .title {
  background-color: #b2d76e;
  padding: 2rpx 15rpx;
  border-radius: 8rpx;
  margin: 10rpx;
}
.user-info .middle-section .level .progress-box {
  height: 50rpx;
  width: 300rpx;
  display: flex;
  align-items: center;
  gap: 20rpx;
}
.user-info .right-section {
  flex: 1;
  display: flex;
  flex-direction: column;
  margin: auto;
  text-align: center;
}
.user-info .right-section .code {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
}
.user-info .right-section text {
  margin-top: 10rpx;
}
.feature1 {
  display: flex;
  gap: 20rpx;
  margin-top: 20rpx;
}
.feature1 .reservation {
  flex: 2;
}
.feature1 .door-to-door {
  flex: 3;
}
.feature1 .reservation,
.feature1 .door-to-door {
  height: 300rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.feature1 .reservation .icon1,
.feature1 .door-to-door .icon1 {
  width: 110rpx;
  height: 110rpx;
  margin: 0 auto;
}
.feature1 .reservation text,
.feature1 .door-to-door text {
  font-weight: bold;
  text-align: center;
  margin-top: 30rpx;
  letter-spacing: 5rpx;
}
.feature2 {
  height: 180rpx;
  margin-top: 20rpx;
  display: flex;
}
.feature2 .pet-boarding,
.feature2 .point-exchange,
.feature2 .coupon {
  display: flex;
  flex-direction: column;
  justify-content: center;
  flex: 1;
}
.feature2 .pet-boarding .icon2,
.feature2 .point-exchange .icon2,
.feature2 .coupon .icon2 {
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
}
.feature2 .pet-boarding text,
.feature2 .point-exchange text,
.feature2 .coupon text {
  font-size: 28rpx;
  text-align: center;
  margin-top: 10rpx;
  letter-spacing: 5rpx;
}
.hot-line {
  height: 100rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  margin: 20rpx 0;
  padding: 40rpx 30rpx;
  display: flex;
}
.hot-line .cocount {
  flex: 3;
  margin: auto 0;
}
.hot-line .brand-hotline {
  flex: 6;
  text-align: right;
  font-weight: bold;
  margin: auto 25rpx auto 0;
}
.hot-line .icon3 {
  flex: 1;
  width: 60rpx;
  height: 60rpx;
  margin: auto 0;
}